//Geek for geeks Tiles

#include "bits/stdc++.h"
using namespace std;
typedef unsigned long long int ull;
#define blank putc_unlocked('\n',stdout);
#define ForIter(it,name) for(it=name.begin();it!=name.end();it++)
#define Forall(i,start,end) for(i=start;i<=end;i++)
#define For(i,start,end) for(i=start;i<end;i++)
struct Job
{
  int start,end1,profit;
};
vector<Job> jobs;

int main()
{
  int n,i,j,k,s,e,p;
  cin>>n;
  For(i,0,n)
  {
    Job j;
    cin>>j.start>>j.end1>>j.profit;
    jobs.push_back(j);
  }
  cout<<"You entered\n";
  For(i,0,n)
  {
    cout<<jobs[i].start<<","<<jobs[i].end1<<","<<jobs[i].profit<<"\n";
  }
  return 0;
}
