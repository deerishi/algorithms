    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "\n",
    "x=[1,3,1,4,6,9,8,7]\n",
    "y=[3,2,1,5,3,9,7,6]\n",
    "d=zip(x,y)\n",
    "l=[]\n",
    "for q in d:\n",
    "    l.append(str(q))\n",
    "print l\n",
    "plt.plot(x,y,'x')\n",
    "k=0\n",
    "for  i,j in d:\n",
    "    print 'i is ',i,' j is ',j\n",
    "    plt.annotate(xy=(i,j),s=l[k])\n",
    "    k+=1\n",
    "    \n",
    "plt.show()"
